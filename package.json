{
  "name": "nti-util-ios-version",
  "version": "1.0.1",
  "description": "Get the iOS version or null if not iOS",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "make test",
    "prepublish": "not-in-install && make || in-install",
    "preversion": "make test",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NextThought/nti.util.ios-version.git"
  },
  "keywords": [
    "ios",
    "version"
  ],
  "publishConfig": {
    "registry": "https://npm.nextthought.com"
  },
  "author": "nextthought",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-cli": "^6.1.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^19.0.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-preset-env": "^1.1.4",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.18.0",
    "eslint-config-nti-codestyle-js": "^1.1.0",
    "in-publish": "^2.0.0",
    "jest": "^19.0.2",
    "jest-junit": "^1.4.0",
    "rollup": "^0.41.5",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^8.0.0",
    "rollup-plugin-json": "^2.0.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/*.spec.js"
    ],
    "coverageDirectory": "reports/coverage",
    "coverageReporters": ["text", "lcov", "cobertura"],
    "testResultsProcessor": "./node_modules/jest-junit"
  },
  "jest-junit": {
    "output": "reports/test-results/index.xml"
  }
}